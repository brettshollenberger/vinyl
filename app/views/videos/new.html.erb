<div class="container" id="twigform">
  <%= form_for @video do |f| %>
    <div>
      <%= f.label :name %><br />
      <%= f.text_field :name, :autofocus => true %>
    </div>
    <div>
      <%= f.label :embed_code %><br \>
      <%= f.text_area :embed_code%>
    </div>
    <div>
      <%= f.submit "Add Video", :class => "twig-btn-inverse btn" %>
    </div>
  <% end %>
</div>
