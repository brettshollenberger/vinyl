<!-- ############################# Main Page ##############################-->
<div class="hero-unit" id="twiggy-unit" data-type="background" data-speed="10">
  <div id="youtube">
    <p>REEL.</p>
    <p class="bg-text">REEL.</p>
    <iframe width="540" height="302" src="http://www.youtube.com/embed/6V9a_pc_F24?feature=player_embedded" frameborder="0" 
    allowfullscreen></iframe>
  </div>
</div>
<!-- ########################## Wave Transition ###########################-->
<div class="wave-container">
  <div id="wave"><img src="/assets/wave_light.png"></div>
</div>
<div class="film" data-type="background" data-speed="10">
  <% if current_user.admin? %>
    <%= link_to "Add Video", new_videos_path, :class => "btn twig-btn-inverse" %>
  <% end %>
  <div class="row">
    <% @videos.each do |video| %>
      <div class="span6">
        <h2><%= video.name %></h2>
        <%= raw "#{video.embed_code}" %>
      </div>
    <% end %>
    
